{
  "name": "rootwork/phalcon-view-string",
  "description": "A Phalcon 2 View library for rendering string templates",
  "license": "BSD-3-Clause",
  "keywords": ["rootwork", "phalcon", "view", "render string"],
  "homepage": "https://github.com/rootworkit/phalcon-view-string",
  "authors": [
    {
      "name": "Mike Soule",
      "email": "mike@rootwork.it"
    }
