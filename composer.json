  "require": {
    "php": ">=5.4",
    "ext-phalcon": ">=2.0.4"
  },
  "require-dev": {
    "phalcon/devtools": "dev-master",
    "phpunit/phpunit": "5.1.*"
  },
  "autoload": {
    "psr-4": { "Rootwork\\Phalcon\\": "src/Phalcon/" }
